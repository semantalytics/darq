PREFIX state: <http://www.daml.ri.cmu.edu/ont/USRegionState.daml#>
PREFIX city:  <http://www.daml.ri.cmu.edu/ont/USCity.daml#>

SELECT ?cityname ?borderstatename {
	?state state:name "New York" .
	?borderstate state:borderstate ?state .
	?borderstate state:name ?borderstatename .
	?city city:state ?borderstate .
	?city city:name ?cityname .
}
ORDER BY DESC(?borderstatename)